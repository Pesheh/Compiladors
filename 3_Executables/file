proc foo is
  type ran is new integer range 1..3;
  
  type matrix is array (ran) of integer;
  proc outern(a: in out integer; d: in matrix) is
    gtc: constant integer:= 1;
    proc inner (b: in out integer; c: in integer) is
    begin
      b:= gtc;
    end proc;
  begin
    inner(a, 5);
end proc;

m: matrix;
k: integer; 
begin
outern(k, m);
end proc;
